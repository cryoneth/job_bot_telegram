# Telegram API Credentials
# Get these from https://my.telegram.org
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash

# Bot Token
# Get this from @BotFather on Telegram
BOT_TOKEN=your_bot_token

# Your Telegram User ID
# Get this from @userinfobot on Telegram
OWNER_USER_ID=your_user_id

# Additional Authorized Users (comma-separated user IDs)
# Other users who can use the bot
AUTHORIZED_USERS=

# CV Encryption Key (auto-generated on first run if not set)
# Store this securely - needed to decrypt your CV
CV_ENCRYPTION_KEY=

# Match Threshold (0-100)
# Jobs with scores below this won't trigger alerts
MATCH_THRESHOLD=70
